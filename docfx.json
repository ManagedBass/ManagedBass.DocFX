{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.sln"
          ],
          "exclude": [
            "**/obj/**",
            "**/iOS/**",
            "**/Desktop/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml"]
      }
    ],
    "resource": [
      {
        "files": ["index.html"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"]
      }
    ],
    "xref": ["xrefmap.zip"],
    "dest": "_site",
    "template": ["default", "docfx-tmpl/src"],
    "noLangKeyword": false
  }
}