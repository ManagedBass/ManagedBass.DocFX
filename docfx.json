{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.cs"
          ],
          "exclude": [
            "**/obj/**",
            "**/iOS.csproj",
            "**/Desktop.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml"]
      }
    ],
    "resource": [
      {
        "files": ["index.html"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"]
      }
    ],
    "xref": ["xrefmap.zip"],
    "dest": "_site",
    "template": ["template"],
    "noLangKeyword": false
  }
}