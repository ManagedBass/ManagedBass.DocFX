{
  "metadata": [
    {
      "src": [
        {
          "cwd": "../",
          "files": ["ManagedBass.PInvoke/src/Windows/Windows.csproj"],
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": ["_site/**", "README.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"]
      }
    ],
    "dest": "_site",
    "template": ["default"],
    "noLangKeyword": false
  }
}