{{!include(/^styles/.*/)}}
<!DOCTYPE html>
<html>
  {{>partials/head}}
  <body data-spy="scroll" data-target="#affix">
    <div id="wrapper">
      <header>
      </header>
      <div role="main" class="container-fluid body-content">
        <div class="sidenav">
          <a class="btn toc-toggle collapse" data-toggle="collapse" href="#sidetoggle" aria-expanded="false" aria-controls="sidetoggle">Show / Hide Table of Contents</a>
          <div class="sidetoggle collapse" id="sidetoggle">
            <div>
              <div class="sidefilter">
                <form>
                  <input type="text" class="form-control" id="toc_filter_input" placeholder="Enter here to filter..." onkeypress="if(event.keyCode==13) {return false;}">
                </form>
              </div>
              <div class="sidetoc">
                <div class="toc" id="toc">
                  {{^leaf}}
                    {{>partials/li}}
                  {{/leaf}}
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="article row grid-right">
          <div class="col-md-10">

              {{^leaf}}
                {{>partials/li}}
              {{/leaf}}
          </div>
        </div>
      </div>
      {{>partials/footer}}
    </div>
    {{>partials/scripts}}
  </body>
</html>